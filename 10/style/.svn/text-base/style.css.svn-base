@charset "utf-8";
/* CSS Document */
*{margin:0;padding:0;-webkit-tap-highlight-color:rgba(255, 255, 255, 0);user-select: none;-webkit-user-select:​ none;-webkit-touch-callout: none;}
.fixed:after {clear:both;content:'.';display:block;font-size:0;line-height:0;visibility:hidden;height:0;}
.fixed {zoom:1;}
::-webkit-input-placeholder,:-moz-placeholder,::-moz-placeholder,:-ms-input-placeholder{color:#999;}
ul,li{list-style-type:none;}
/* 外边距 */
