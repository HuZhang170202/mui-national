.mtb20{margin-top:20px;margin-bottom:20px;}
.mlr20{margin-left:20px;margin-right:20px;}

.al-c{text-align: center;}
.al-l{text-align: left;}
.al-r{text-align: right;}

.fl{float: left;}
.fr{float: right;}

.c-red{
	color: #CE4031;
}
.c-yellow{color: #f6cc45}
.opaCity{opacity: 0;}
.boxSiz{
	box-sizing: border-box;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	-ms-box-sizing: border-box;
	-o-box-sizing: border-box;
}
.fz-30{font-size: 30px;}
.ft-b{font-weight: bold;}
.clearfix:after {
	content: "";
	display: block;
	visibility: hidden;
	height: 0;
	clear: both;
}
.clearfix {
	zoom: 1;
}

.page{
	max-width: 750px;
	margin: 0 auto;
}
.main{
	height:100%;
	overflow:hidden;
	margin:0 auto;
	position:relative;
	-webkit-transform-origin:center top;
	transform-origin:center top;
	background-color: #fff;
	background-size: 100% 100%;
}
.page-num{
	font-size: 30px;
	font-weight: bold;
	color: #333;
	position: absolute;
	left: 50%;
	margin-left: -10px;
	bottom: 20px;
	z-index: 8888;
	opacity: 0;
}
/*第一屏-----*/
.music_inco{
	position:absolute;
	top:10px;
	left:20px;
	z-index:2;
	background:url(../images/music-inco02.png) no-repeat scroll center center;
	width:50px;
	height:50px;
	animation:run 2s infinite 0s linear;
	-webkit-animation:run 2s infinite 0s linear;
}
.loading_box{
	background:#fff;
	width:100%;
	height:100%;
	position:fixed;
	top:0;
	left:0; 
	z-index:999; 
}
.loading_logo{
	position:absolute;
	top:50%;
	left:50%;
	transform:translate(-50%,-50%);
	-webkit-transform:translate(-50%,-50%);
}
.pages{
	position:absolute;
	width:100%;
	height:100%;
	/*min-height:1008px;*/
	top:50%;
	left:0;
	-webkit-transform:translate(0,-50%);
	transform:translate(0,-50%);
}
.bg1,.bg2,.bg3,.bg4,.bg5,.bg6,.bg7,.bg8,.bg9,.bg10,.bg11,.bg12,.bg13,.bg14,.bg15,.bg16{
	position: absolute;
	top: 0;left: 0px;
	width: 100%;
	height: 100%;
	background-color: #fcf0e0;
	background-size: 100% 100%;
}
.p1,.p2,.p3,.p4,.p5,.p6,.p7,.p8,.p9,.p10,.p11,.p12,.p13,.p14{
	position: absolute;
	width: 100%;
	height: 40px;
	background-color: pink;
	top: 50%;
	left: 0;
}

